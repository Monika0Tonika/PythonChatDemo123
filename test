from tkinter import *
from PIL import ImageTk, Image

root=Tk()

root.title("Test")
root.iconbitmap("C:/Users/montut/OneDrive - RaySearch Laboratories AB/Desktop/Python VSC")

my_img= ImageTk.PhotoImage(Image.open("images.png"))
mylabel= Label(image=my_img)
mylabel.pack()





button_quit=Button(root, text="Exit", command=root.quit)
button_quit.pack()

root.mainloop()







